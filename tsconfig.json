{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@env-feelback-app-api/environment": [
        "apps/feelback-api/src/environments/environment.ts"
      ],
      "@env-feelback-app-web/environment": [
        "apps/feelback-web/src/environments/environment.ts"
      ],
      "@feelback-app/api/auth": ["libs/api/auth/src/index.ts"],
      "@feelback-app/api/config": ["libs/api/config/src/index.ts"],
      "@feelback-app/api/core": ["libs/api/core/src/index.ts"],
      "@feelback-app/api/data": ["libs/api/data/src/index.ts"],
      "@feelback-app/api/database": ["libs/api/database/src/index.ts"],
      "@feelback-app/api/doctor": ["libs/api/doctor/src/index.ts"],
      "@feelback-app/api/errors": ["libs/api/errors/src/index.ts"],
      "@feelback-app/api/faq": ["libs/api/faq/src/index.ts"],
      "@feelback-app/api/identity": ["libs/api/identity/src/index.ts"],
      "@feelback-app/api/instrument": ["libs/api/instrument/src/index.ts"],
      "@feelback-app/api/interfaces": ["libs/api/interfaces/src/index.ts"],
      "@feelback-app/api/organization": ["libs/api/organization/src/index.ts"],
      "@feelback-app/api/person": ["libs/api/person/src/index.ts"],
      "@feelback-app/api/screening": ["libs/api/screening/src/index.ts"],
      "@feelback-app/api/shared": ["libs/api/shared/src/index.ts"],
      "@feelback-app/api/testing": ["libs/api/testing/src/index.ts"],
      "@feelback-app/api/user": ["libs/api/user/src/index.ts"],
      "@feelback-app/api/util": ["libs/api/util/src/index.ts"],
      "@feelback-app/features/ui": ["libs/features/ui/src/index.ts"],
      "@feelback-app/shared/content": ["libs/shared/content/src/index.ts"],
      "@feelback-app/shared/misc": ["libs/shared/misc/src/index.ts"],
      "@feelback-app/shared/style": ["libs/shared/style/src/index.ts"],
      "@feelback-app/util/connection": ["libs/util/connection/src/index.ts"],
      "@feelback-app/util/helper": ["libs/util/helper/src/index.ts"],
      "@feelback-app/util/testing": ["libs/util/testing/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
