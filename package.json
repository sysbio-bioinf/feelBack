{
  "name": "cancerlog",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "prebuild": "npm run clean:dist && npm run gql-codegen",
    "build": "ng build",
    "build.feelback.ionic": "nx build feelback-ionic",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "clean.feelback.ionic": "cd apps/feelback-ionic && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf documentation",
    "cmd:feelback": "npx ts-node --project apps/feelback-api/tsconfig.app.json -r tsconfig-paths/register ./apps/feelback-api/src/cli.ts",
    "dep-graph": "nx dep-graph",
    "deps:bump": "npx ncu -iu",
    "docker:dev:serve": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "docker:dev:stop": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml stop",
    "docker:prod:build": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build",
    "docker:prod:serve": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:prod:stop": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml stop",
    "docs:build": "npm run docs:build:feelback",
    "docs:build:feelback": "npx compodoc -p apps/feelback-api/tsconfig.app.json -d documentation/feelback",
    "docs:serve": "npx compodoc -s -o",
    "e2e": "ng e2e",
    "env:sync": "npx sync-dotenv",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:package": "npx sort-package-json",
    "format:write": "nx format:write",
    "gql-codegen": "graphql-codegen --config gql-codegen.yml",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "lint": "nx workspace-lint && ng lint",
    "migration:create": "bash -c 'cd apps/${1}/src && npx typeorm migration:create -n ${2}' -- ",
    "migration:run": "bash -c 'cd apps/${1}/src && npx tsc database/migrations/src/*.ts --outDir database/migrations/generated && npx typeorm migration:run' -- ",
    "ng": "ng",
    "nx": "nx",
    "open.feelback.ionic.android": "cd apps/feelback-ionic && npm run cap.android",
    "open.feelback.ionic.ios": "cd apps/feelback-ionic && npm run cap.ios",
    "prepare.feelback.ionic": "npm run clean && npm run clean.feelback.ionic && npm run build.feelback.ionic",
    "prepare.feelback.ionic.android": "npm run prepare.feelback.ionic && cd apps/feelback-ionic && npm run cap.add.android",
    "prepare.feelback.ionic.ios": "npm run prepare.feelback.ionic && cd apps/feelback-ionic && npm run cap.add.ios",
    "start": "ng serve",
    "sync.feelback.ionic": "cd apps/feelback-ionic && npm run cap.copy",
    "test": "ng test",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
