{
  "name": "cancerlog",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "prebuild": "npm run clean:dist && npm run gql-codegen",
    "build": "ng build",
    "build.ionic.feelback": "nx build ionic-feelback",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "clean.ionic.feelback": "cd apps/ionic-feelback && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf documentation",
    "cmd:feelback": "npx ts-node --project apps/feelback-api/tsconfig.app.json -r tsconfig-paths/register ./apps/feelback-api/src/cli.ts",
    "dep-graph": "nx dep-graph",
    "deps:bump": "npx ncu -iu",
    "docker:build:api": "docker build -f .docker/api.Dockerfile -t cancerlog.api:latest .",
    "docker:build:core": "docker build -f .docker/core.Dockerfile -t cancerlog.core:latest .",
    "docker:build:deps": "docker build -f .docker/deps.Dockerfile -t cancerlog.deps:latest .",
    "docs:build": "npm run docs:build:feelback",
    "docs:build:feelback": "npx compodoc -p apps/feelback-api/tsconfig.app.json -d documentation/feelback",
    "docs:serve": "npx compodoc -s -o",
    "e2e": "ng e2e",
    "env:sync": "npx sync-dotenv",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:package": "npx sort-package-json",
    "format:write": "nx format:write",
    "gql-codegen": "graphql-codegen --config gql-codegen.yml",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "lint": "nx workspace-lint && ng lint",
    "migration:create": "bash -c 'cd apps/${1}/src && npx typeorm migration:create -n ${2}' -- ",
    "migration:run": "bash -c 'cd apps/${1}/src && npx tsc database/migrations/src/*.ts --outDir database/migrations/generated && npx typeorm migration:run' -- ",
    "ng": "ng",
    "nx": "nx",
    "open.ionic.feelback.android": "cd apps/ionic-feelback && npm run cap.android",
    "open.ionic.feelback.ios": "cd apps/ionic-feelback && npm run cap.ios",
    "prepare.ionic.feelback": "npm run clean && npm run clean.ionic.feelback && npm run build.ionic.feelback",
    "prepare.ionic.feelback.android": "npm run prepare.ionic.feelback && cd apps/ionic-feelback && npm run cap.add.android",
    "prepare.ionic.feelback.ios": "npm run prepare.ionic.feelback && cd apps/ionic-feelback && npm run cap.add.ios",
    "start": "ng serve",
    "sync.ionic.feelback": "cd apps/ionic-feelback && npm run cap.copy",
    "test": "ng test",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
