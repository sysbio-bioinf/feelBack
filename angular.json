{
  "version": 1,
  "projects": {
    "feelback-web": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/feelback-web",
      "sourceRoot": "apps/feelback-web/src",
      "prefix": "cancerlog",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/feelback-web",
            "index": "apps/feelback-web/src/index.html",
            "main": "apps/feelback-web/src/main.ts",
            "polyfills": "apps/feelback-web/src/polyfills.ts",
            "tsConfig": "apps/feelback-web/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/feelback-web/src/favicon.ico",
              "apps/feelback-web/src/assets"
            ],
            "styles": ["apps/feelback-web/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/feelback-web/src/environments/environment.ts",
                  "with": "apps/feelback-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "feelback-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "feelback-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "feelback-web:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/feelback-web/tsconfig.app.json",
              "apps/feelback-web/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/feelback-web/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/feelback-web/jest.config.js",
            "tsConfig": "apps/feelback-web/tsconfig.spec.json",
            "setupFile": "apps/feelback-web/src/test-setup.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "feelback-web-e2e": {
      "root": "apps/feelback-web-e2e",
      "sourceRoot": "apps/feelback-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/feelback-web-e2e/cypress.json",
            "tsConfig": "apps/feelback-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "feelback-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "feelback-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/feelback-web-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/feelback-web-e2e/**"]
          }
        }
      }
    },
